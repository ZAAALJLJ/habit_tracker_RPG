<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">
    <style name="CustomFontStyle">
        <item name="android:fontFamily">@font/font2</item>
        <item name="android:textSize">6sp</item>
        <!-- Add other styling attributes as needed -->
    </style>
    <style name="BottomNavigationTextStyle">
        <item name="android:textSize">9dp</item> <!-- Adjust the text size as needed -->
    </style>
    <style name="SmallFabShape">
        <item name="android:background">@drawable/addtask</item>
    </style>

    <style name="SmallFabShapeAppearance" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">0dp</item>
        <item name="backgroundTint">@android:color/transparent</item>
    </style>

    <style name="SplashTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowFullscreen">true</item>
    </style>

</resources>